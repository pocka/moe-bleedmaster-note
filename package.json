  "private": true,
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts-ts": "2.16.0",
    "styled-components": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
